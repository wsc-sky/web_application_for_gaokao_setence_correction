{
  "vars": {
    "@font-family-sans-serif": "'Lato', sans-serif",
    "@font-size-base": "16px",
    "@font-size-h1": "floor(@font-size-base * 2.3)",
    "@font-size-h2": "floor(@font-size-base * 1.9)",
    "@font-size-h3": "ceil(@font-size-base * 1.5)",
    "@font-size-h4": "ceil(@font-size-base * 1.1)",
    "@font-size-h6": "ceil(@font-size-base * 0.7)",
